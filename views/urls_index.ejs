<body>
  <main style="margin: 1em;">
    <h3>My URLS</h3>
    <table class="table">
      <thead>
        <tr>
          <th scope="col">Short URL ID</th>
          <th scope="col">Long URL ID</th>
        </tr>        
      </thead>
      <tbody>
        <% for (let id of urls) { %>
        <tr>
          <td><%= id %></td>
          <td> <%= urls[id] %></td>
        </tr>
        <% } %>
      </tbody>
    </table>
  </main>
